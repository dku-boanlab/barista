# l2_shortest.conf

#    name = application name
#    args = arguments
#    type = [general|autonomous]
#    site = [internal|external]
#    role = [network|security|admin]
#    perm = [r|w|x]
#           r: receive inbounds
#           w: can modify incoming event data
#           x: can cut control flows among components
#    status = [enabled|disabled]
#    inbounds = APP_EVENT
#    outbounds = APP_EVENT

[

{
    "name":"l2_shortest",
    "type":"general",
    "site":"external",
    "role":"network",
    "perm":"r",
    "status":"enabled",
    "inbounds":["AV_DP_RECEIVE_PACKET",
                "AV_DP_PORT_DELETED",
                "AV_SW_CONNECTED",
                "AV_SW_DISCONNECTED",
                "AV_LINK_ADDED",
                "AV_LINK_DELETED"],
    "outbounds":["AV_DP_SEND_PACKET",
                 "AV_DP_INSERT_FLOW"]
}

]
